from py_ballisticcalc.trajectory_data import TrajectoryData

__all__ = ['TrajectoryCalc']

class TrajectoryCalc:
    def __init__(self, _config: Config) -> None: ...
    @property
    def table_data(self) -> list[DragDataPoint]: ...
